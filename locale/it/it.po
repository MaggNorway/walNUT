#. TRANSLATORS: UPS Command description
#: ../utilities.js:490
msgid "Turn off the load immediately"
msgstr "Spegni il carico immediatamente"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:496
msgid "Turn on the load immediately"
msgstr "Accendi il carico immediatamente"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:502
msgid "Turn off the load possibly after a delay"
msgstr "Spegni il carico possibilmente dopo un ritardo"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:508
msgid "Turn on the load possibly after a delay"
msgstr "Accendi il carico possibilmente dopo un ritardo"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:514
msgid "Turn off the load possibly after a delay and return when power is back"
msgstr "Spegni il carico possibilmente dopo un ritardo e riaccendilo quando la corrente è tornata"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:520
msgid "Turn off the load possibly after a delay and remain off even if power returns"
msgstr "Spegni il carico possibilmente dopo un ritardo e rimani spento anche se la corrente ritorna"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:526
msgid "Stop a shutdown in progress"
msgstr "Annulla un arresto in corso"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:532
msgid "Shut down the load briefly while rebooting the UPS"
msgstr "Spegni il carico brevemente mentre si riavvia l'UPS"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:538
msgid "After a delay, shut down the load briefly while rebooting the UPS"
msgstr "Dopo un ritardo, spegni il carico brevemente mentre si riavvia l'UPS"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:544
msgid "Start testing the UPS panel"
msgstr "Inizia un test del pannello dell'UPS"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:550
msgid "Stop a UPS panel test"
msgstr "Ferma un test del pannello dell'UPS"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:556
msgid "Start a simulated power failure"
msgstr "Inizia una simulazione d'interruzione di corrente "

#. TRANSLATORS: UPS Command description
#: ../utilities.js:562
msgid "Stop simulating a power failure"
msgstr "Ferma la simulazione d'interruzione di corrente"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:568
msgid "Start a battery test"
msgstr "Inizia un test sulla batteria"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:574
msgid "Start a \"quick\" battery test"
msgstr "Inizia un test \"veloce\" sulla batteria"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:580
msgid "Start a \"deep\" battery test"
msgstr "Inizia un test \"profondo\" sulla batteria"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:586
msgid "Stop the battery test"
msgstr "Ferma il test sulla batteria"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:592
msgid "Start runtime calibration"
msgstr "Inizia una calibrazione dell'autonomia"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:598
msgid "Stop runtime calibration"
msgstr "Ferma la calibrazione dell'autonomia"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:604
msgid "Put the UPS in bypass mode"
msgstr "Metti l'UPS in modalità bypass"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:610
msgid "Take the UPS out of bypass mode"
msgstr "Togli l'UPS dalla modalità bypass"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:616
msgid "Reset minimum and maximum input voltage status"
msgstr "Azzera lo stato del minimo e del massimo voltaggio in ingresso"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:622
msgid "Reset watchdog timer (forced reboot of load)"
msgstr "Azzera il timer del watchdog (riavvio forzato del carico)"

#. TRANSLATORS: UPS Command description
#: ../utilities.js:628
msgid "Enable UPS beeper/buzzer"
msgstr "Abilita il segnale acustico dell'UPS"

#. TRANSLATORS: UPS Command description
